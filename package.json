		"@types/node": "^22.13.4",
		"builtin-modules": "4.0.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"he": "^1.2.0",
		"mime": "^4.0.1"
	}
